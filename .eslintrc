{
    "parserOptions": {
      "ecmaVersion": 10
    },
    "rules": {
      "no-undef": "off",
      "semi": ["error", "never"],
      "array-bracket-spacing": [
        "error",
        "never"
      ],
      "no-trailing-spaces": "error",
      "comma-spacing": [
        "error",
        {
          "before": false,
          "after": true
        }
      ],
      "indent": [
        "error",
        2
      ],
      "key-spacing": [
        "error",
        {
          "beforeColon": false,
          "afterColon": true
        }
      ]
    },
    "env": {
      "node": true
    },
    "extends": "eslint:recommended"
  }